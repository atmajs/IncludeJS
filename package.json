{
  "name": "includejs",
  "description": "Resource Loader",
  "version": "0.12.47",
  "homepage": "http://atmajs.com/include",
  "contributors": [],
  "author": "Alex Kit <alex.kit@atmajs.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/tenbits/IncludeJS.git"
  },
  "bugs": {
    "url": "https://github.com/tenbits/IncludeJS/issues"
  },
  "licenses": [
    "MIT"
  ],
  "main": "./lib/include.node.js",
  "dependencies": {},
  "devDependencies": {
    "atma": ">0.9.0",
    "atma-loader-babel": "^1.0.21"
  },
  "scripts": {
    "test": "atma test"
  },
  "keywords": [],
  "engines": {
    "node": "~0.8.9"
  },
  "atma": {
    "plugins": [
      "atma-loader-babel"
    ],
    "settings": {
      "io": {
        "extensions": {
          "js": ["condcomments:read", "importer:read"]
        }
      },
      "atma-loader-babel": {
        "extensions": [
          "es6",
          "test"
        ]
      }
    }
  }
}
